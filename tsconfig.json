{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "~environments/*": ["environments/*"],
      "~auth/*": ["modules/auth/*"],
      "~main/*": ["modules/main/*"],
      "~login/*": ["modules/login/*"],
      "~dashoard/*": ["modules/dashboard/*"],
      "~store/*": ["modules/store/*"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
